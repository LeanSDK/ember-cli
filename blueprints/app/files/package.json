{
  "name": "<%= name %>",
  "version": "0.0.0",
  "private": true,
  "description": "LeanES application for <%= name %> goes here",
  "repository": "",
  "license": "AGPL-3.0",
  "author": "",
  "directories": {
    "app": "app",
    "configs": "configs",
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "watch": "broccoli serve --output-path dev",
    "dev": "nodemon -L --delay 10ms --watch dev/ --require source-map-support/register --trace-warnings --inspect=0.0.0.0:9229 index.js",
    "dev_build": "broccoli build --output-path dev",
    "dev_migrate": "ENV=development node --require source-map-support/register --trace-warnings migrate",
    "dev_rollback": "ENV=development node --require source-map-support/register --trace-warnings rollback",
    "build": "broccoli build --prod --output-path prod",
    "migrate": "node migrate",
    "rollback": "node rollback",
    "start": "pm2 start index.js",
    "pm2": "pm2",
    "test": "mocha",
    "test_build": "ENV=build mocha"
  },
  "contributors": [],
  "keywords": [
    "MVC",
    "PureMVC",
    "LeanES",
    "leanes-app"
  ],
  "configuration": {
    "port": {
      "description": "port for start example server",
      "type": "number",
      "default": 3001
    },
    "silent": {
      "description": "silent console logs",
      "type": "boolean",
      "default": false
    },
    "trustProxy": {
      "description": "trustProxy for start example server",
      "type": "boolean",
      "default": false
    },
    "subdomainOffset": {
      "description": "subdomainOffset for start example server",
      "type": "number",
      "default": 2
    },
    "sessionCookie": {
      "description": "Session cookie name.",
      "type": "string",
      "default": "sid"
    },
    "resetTokenTTL": {
      "description": "Reset token time-to-live (in seconds)",
      "type": "number",
      "default": 86400
    },
    "adminEmail": {
      "description": "Admin email.",
      "type": "string",
      "default": "admin@leanes.com"
    },
    "adminPassword": {
      "description": "Admin password.",
      "type": "string",
      "default": "0000"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-decorators": "^7.10.4",
    "@babel/plugin-transform-flow-strip-types": "^7.9.0",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.11.5",
    "babel-plugin-flow-runtime": "^0.19.0",
    "babel-plugin-globals": "^3.0.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-parameter-decorator": "1.0.16",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "broccoli": "^3.4.2",
    "broccoli-babel-transpiler": "7.8.0",
    "broccoli-cli": "1.0.0",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-source": "3.0.0",
    "broccoli-string-replace": "0.1.2",
    "chai": "4.2.0",
    "flow-runtime": "^0.17.0",
    "glob": "7.1.6",
    "lodash": "^4.17.19",
    "mocha": "^8.1.3",
    "sinon": "^9.0.3",
    "sinon-chai": "3.5.0",
    "source-map-support": "^0.5.19"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@leansdk/leanes": "^7.0.0",
    "@leansdk/leanes-fs-utils-addon": "^7.0.0",
    "@leansdk/leanes-configurable-addon": "^7.0.0",
    "@leansdk/leanes-mapper-addon": "^7.0.0",
    "@leansdk/leanes-mongo-addon": "^7.0.0",
    "@leansdk/leanes-restful-addon": "^7.0.0",
    "@leansdk/leanes-queryable-addon": "^7.0.0",
    "@leansdk/leanes-swagger-addon": "^7.0.0",
    "commander": "^6.2.0",
    "json-joi-converter": "^17.2.1-a",
    "nodemon": "1.19.4",
    "pm2": "^3.5.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  }
}
